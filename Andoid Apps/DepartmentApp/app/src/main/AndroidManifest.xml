<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shubhangi.departmentapp">
    <uses-permission android:name="android.permission.READ_SMS"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="shubhangi.com.departmentapp.Container"
        android:label="@string/app_name">
        <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="shubhangi.com.departmentapp.MainActivity">
        </activity>
        <activity android:name="shubhangi.com.departmentapp.Feedback">
        </activity>
        <activity android:name="shubhangi.com.departmentapp.FeedbackResolve"></activity>
    </application>

</manifest>